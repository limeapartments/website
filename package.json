{
  "name": "limeapartments",
  "version": "1.0.0",
  "description": "A rewrite of the limeapartments website.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./build || true",
    "build": "npm run clean && webpack",
    "dev": "webpack-dev-server --config webpack.config.js",
    "build:production": "webpack --config webpack.prod.js",
    "build:docs": "npm run build:production && cp -r static/ docs/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limeapartments/website.git"
  },
  "author": "Chance Hudson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/limeapartments/website/issues"
  },
  "homepage": "https://github.com/limeapartments/website#readme",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "extraFileExtensions": [
        ".tsx"
      ],
      "ecmaFeatures": {
        "jsx": true
      },
      "useJSXTextNode": true,
      "project": "./tsconfig.json"
    },
    "extends": [
      "@jchancehud/eslint-config"
    ]
  },
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@jchancehud/eslint-config": "0.0.8",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.11",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.14.1",
    "file-loader": "^3.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "styled-components": "^4.1.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
